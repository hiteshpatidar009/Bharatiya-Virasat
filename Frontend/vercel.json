{
  "builds": [
    {
      "src": "Frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/Frontend/" }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/Frontend/" }
  ]
}